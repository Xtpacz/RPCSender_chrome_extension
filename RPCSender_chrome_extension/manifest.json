{
  "manifest_version": 3,
  "name": "RPC Sender",
  "version": "1.0.0",
  "description": "一个用于发送HTTP请求的Chrome插件。",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "32": "icon32.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
} 